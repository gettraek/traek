		"publint": "^0.3.17",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"keywords": ["svelte", "svelte5", "ai", "chat", "agent", "tree", "conversation"],
	"repository": {
