		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest run",
		"test": "vitest run"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@iconify/svelte": "^5.2.1",
		"dompurify": "^3.3.1",
		"highlight.js": "^11.11.1",
		"marked": "^17.0.1",
		"marked-highlight": "^2.2.3",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@testing-library/svelte": "^5.3.1",
		"@types/node": "^24",
		"eslint": "^9.39.2",
		"jsdom": "^28.1.0",
