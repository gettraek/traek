{
	"name": "@traek/sdk",
	"version": "0.0.2",
	"description": "Spatial tree-chat UI for AI agents (Svelte 5)",
	"author": "Nico HÃ¼lscher",
	"license": "MIT",
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./server": {
			"types": "./src/server/rate-limit.ts",
			"default": "./src/server/rate-limit.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": ["**/*.css"],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && svelte-kit sync && svelte-package && publint",
